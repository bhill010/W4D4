<h1> Edit <%= @band.name %></h1>

<form action="<%= band_url(@band) %>" method="post">
  <input type="hidden"
  name="_method"
  value= "PATCH" />

  <input type="hidden"
  name="authenticity token"
  value= "<%= form_authenticity_token %>" />

  <label for="band_name">Name</label>
  <input
  type = "text"
  name = "band[name]"/>

  <input type = "submit" value="Submit" />
</form>
