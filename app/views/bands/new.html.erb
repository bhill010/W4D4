<h1> Create Band </h1>

<form action="<%= bands_url %>" method="post">
  <input type="hidden"
  name="authenticity token"
  value= "<%= form_authenticity_token %>" />

  <label for="band_name">Enter band name</label>
  <input
  type = "text"
  name = "band[name]"/>

  <input type = "submit" value="Submit" />
</form>
